# Configurações do Sistema de Fila de Tarefas

# Número máximo de tentativas de retry
MAX_RETRIES=3

# Timeout em segundos para cada tarefa
TASK_TIMEOUT=5

# Nível de logging (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Número de workers para processar tarefas em paralelo
NUM_WORKERS=1

# Intervalo em segundos para verificar novas tarefas
QUEUE_CHECK_INTERVAL=1
